{
  "id": "gatsby-preview-sidebar",
  "name": "Gatsby Preview",
  "srcdoc": "./build/index.html",
  "sidebar": true,
  "parameters": {
    "instance": [
      {
        "id": "contentTypeSlug",
        "type": "Symbol",
        "name": "URL slug for content type"
      }
    ],
    "installation": [
      {
        "id": "previewUrl",
        "type": "Symbol",
        "name": "The url of your Gatsby Preview instance"
      },
      {
        "id": "webhookUrl",
        "type": "Symbol",
        "name": "The url of the webhook to rebuild your Gatsby Preview instance"
      },
      {
        "id": "authToken",
        "type": "Symbol",
        "name": "Optional Authentication token if your Gatsby Preview instance is not public"
      }
    ]
  }
}
