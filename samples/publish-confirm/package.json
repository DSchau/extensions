{
  "name": "publish-with-confirmation",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html",
    "build": "rimraf build/* && parcel build src/index.js -d ./build --no-source-maps && node build.js && rimraf build/index.js && rimraf build/index.css",
    "extension:create": "contentful extension create",
    "extension:update": "contentful extension update --force",
    "extension:local": "contentful extension update --force --src http://localhost:1234/"
  },
  "dependencies": {
    "@contentful/forma-36-fcss": "0.0.11",
    "@contentful/forma-36-react-components": "2.4.1",
    "contentful-ui-extensions-sdk": "3.5.0",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "relative-date": "1.1.3"
  },
  "browserslist": [
    "last 5 Chrome version",
    "> 1%",
    "not ie <= 11"
  ],
  "devDependencies": {
    "@babel/core": "7.3.3",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/plugin-transform-runtime": "7.2.0",
    "parcel": "^1.11.0",
    "rimraf": "^2.6.3"
  }
}
